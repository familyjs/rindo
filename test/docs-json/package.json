{
  "name": "json-docs-testbed",
  "version": "1.0.0",
  "description": "A test app for the docs-json output target",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "node ../../bin/rindo build && npm run postprocess",
    "postprocess": "node scripts/postprocess.js",
    "build.dev": "node ../../bin/rindo build --dev",
    "start": "node ../../bin/rindo build --dev --watch --serve",
    "test": "node ../../bin/rindo test --spec --e2e",
    "test.watch": "node ../../bin/rindo test --spec --e2e --watch",
    "generate": "node ../../bin/rindo generate"
  },
  "license": "MIT"
}
